#include <iostream>  
#include <string>  
#include <vector>

using namespace std;
//返回一个动态分配的vector 
vector<int>* vectorr_i()
{

	vector<int>* p = new vector<int>;
	return p;
}

//给vector赋值
void vector_j(vector<int>* p)
{
	int INT;
	while (cin >> INT)
	{
		p->push_back(INT);
	}
}

//打印vector的值
void vector_k(vector<int>* p)
{
	for (int i = 0; i < p->size(); i++)
	{
		cout << (*p)[i] << endl;
	}
}

int main()
{
	vector<int>* p = vectorr_i();
	vector_j(p);
	vector_k(p);
	delete p;
	return 0;
}
